{
  "manifest_version": 2,
  "name": "Test Extension",
  "version": "0.2.0",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "page": "index.html#/background",
    "persistent": false
  },
  "browser_action": {
    "default_title": "Pool hopping",
    "default_popup": "index.html#/"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}